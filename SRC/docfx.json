{
	"metadata": [{
		"src": [{
			"files": [ "Primitives.csproj" ],
			"exclude": ["**/bin/**", "**/obj/**"]
		}],
		"dest": "../BIN/DOC",
		"properties": {
			"TargetFramework": "netstandard2.0"
		}
	}],
	"build": {
		"content": [
			{
				"files": ["*.yml"],
				"src": "../BIN/DOC"
			},
			{
				"files": ["index.md"]
			}
		],
		"dest": "../Artifacts/DOC",
		"resource": [],
		"globalMetadata": {
			"_appTitle": "Primitives",
			"_disableNavbar": true,
			"_disableContribution": true
		},
		"xrefService": [
			"https://xref.docs.microsoft.com/query?uid={uid}"
		]
	}
}